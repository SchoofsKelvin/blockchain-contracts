{
    "$schema": "http://json.schemastore.org/prettierrc",
    "overrides": [
        {
            "files": "*.sol",
            "options": {
                "printWidth": 160,
                "tabWidth": 4,
                "useTabs": true,
                "singleQuote": false,
                "bracketSpacing": true,
                "explicitTypes": "always"
            }
        }
    ]
}